<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.huatek.hbwebsite.company.entity">
	<class name="CompanyCustomerServiceType" table="company_customer_service_type">
		<id name="id" type="java.lang.Long" column="type_no">
			<generator class="native"/>
		</id>
		<property name="typeName" column="type_name" type="java.lang.String" length="50"/>
		<property name="createTime" column="create_time" type="java.util.Date"/>
		<property name="deleteFlag" column="deleteflag" type="java.lang.Integer"/>
	</class>
</hibernate-mapping>
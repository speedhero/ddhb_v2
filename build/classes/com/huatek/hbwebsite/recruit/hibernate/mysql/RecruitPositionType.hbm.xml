<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.huatek.hbwebsite.recruit.entity.RecruitPositionType" table="recruit_position_type">
		<id name="id" type="java.lang.Long" column="ID">
			<generator class="native">
			</generator>
		</id>
		
		<property name="typeName" column="typename" type="java.lang.String" length="50"></property>
		
		<property name="createTime" column="createtime" type="java.sql.Timestamp"></property>
		
		<property name="deleteFlag" column="deleteflag" type="java.lang.Integer"></property>
		
	</class>
</hibernate-mapping>

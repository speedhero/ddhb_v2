<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.huatek.hbwebsite.common.entity">
	<class name="HouseRate" table="common_loan_rate">
		<id name="id" type="java.lang.Long" column="rate_id">
			<generator class="native"/>
		</id>
		<property name="rateType" column="rate_type" type="java.lang.Integer"/>
		<property name="deleteFlag" column="deleteflag" type="java.lang.Integer"/>
		<property name="rateVal" column="rate_val" type="java.lang.Double"/>
		<property name="rateDate" column="rate_date" type="java.util.Date"/>
	</class>
</hibernate-mapping>
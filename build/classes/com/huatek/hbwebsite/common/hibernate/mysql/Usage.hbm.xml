<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.huatek.hbwebsite.common.entity">
	<class name="Usage" table="common_usage">
		<id name="erpId" type="java.lang.String" column="erp_id">
		</id>
		<property name="usageName" column="usage_name" type="java.lang.String" length="50"/>
		<property name="module" column="module_id" type="java.lang.Integer"/>
		<property name="deleteFlag" column="deleteflag" type="java.lang.Integer"/>
	</class>
</hibernate-mapping>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.huatek.hbwebsite.member.entity.IntegrateRule" table="member_integrate_rule">
		<id name="id" type="java.lang.Long" column="rule_no">
			<generator class="native">
			</generator>
		</id>
		<property name="ruleName" type="java.lang.String">
			<column name="rule_name">
			</column>
		</property>
		<property name="comment" type="java.lang.String">
			<column name="comment">
			</column>
		</property>
		<property name="ruleValue" type="java.lang.Integer">
			<column name="rule_value">
			</column>
		</property>
		<property name="deleteFlag" type="java.lang.Integer">
			<column name="deleteflag">
			</column>
		</property>
	</class>
</hibernate-mapping>
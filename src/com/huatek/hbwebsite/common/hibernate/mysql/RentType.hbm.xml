<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.huatek.hbwebsite.common.entity">
	<class name="RentType" table="common_rent_type">
		<id name="id" type="java.lang.Long" column="rent_type_no">
			<generator class="native"/>
		</id>
		
		<property name="rentTypeName" column="rent_type_name" type="java.lang.String" length="10"/>
		
		<property name="rentValue" column="rent_value" type="java.lang.Integer"/>
		
		<property name="deleteFlag" column="deleteflag" type="java.lang.Integer"/>
		
	</class>
</hibernate-mapping>
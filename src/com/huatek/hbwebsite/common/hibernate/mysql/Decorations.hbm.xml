<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.huatek.hbwebsite.common.entity">
	<class name="Decorations" table="common_decoration_type">
		<id name="erpId" type="java.lang.String" column="erp_id">
		</id>
		<property name="decorationName" column="decoration_name" type="java.lang.String" length="50"/>

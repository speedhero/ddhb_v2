<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.huatek.hbwebsite.common.entity">
	<class name="QuestionStrategyType" table="common_question_strategy_type">
		<id name="erpId" type="java.lang.String" column="erp_id">
		</id>
		<property name="typeName" column="type_name" type="java.lang.String"/>
		<property name="iconUrl" column="icon_url" type="java.lang.String" length="250"/>
		<property name="hoverIconUrl" column="hover_icon_url" type="java.lang.String" length="250"/>
		<property name="createTime" column="create_time" type="java.util.Date"/>
		<property name="deleteFlag" column="deleteflag" type="java.lang.Integer"/>
	</class>
</hibernate-mapping>
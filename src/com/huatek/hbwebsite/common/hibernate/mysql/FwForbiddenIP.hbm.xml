<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.huatek.hbwebsite.common.entity">
	<class name="FwForbiddenIP" table="fw_forbidden_ip">
		<id name="id" type="java.lang.Long" column="id">
			<generator class="native">
	        </generator>
		</id>
		
		<property name="ipAddress" column="ip_address" type="java.lang.String" length="60"></property>
		
		<property name="createTime" column="create_time" type="java.util.Date"></property>
		
		<property name="deleteFlag" column="delete_flag" type="java.lang.Integer"></property>
		
	</class>
</hibernate-mapping>

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.huatek.hbwebsite.common.entity">
	<class name="Orientations" table="common_orientations">
		<id name="id" column="orientation_no" type="java.lang.Long">
			<generator class="native"/>
		</id>
		<property name="orientationName" column="orientation_name" type="java.lang.String" length="10"/>
		<property name="deleteFlag" column="deleteflag" type="java.lang.Integer"/>
	</class>
	
	
</hibernate-mapping>